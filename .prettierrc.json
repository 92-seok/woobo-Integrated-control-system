{
    "phpVersion": "8.2",
    "plugins": ["@prettier/plugin-php"],
    "useTabs": false,
    "tabWidth": 4,
    "singleQuote": true,
    "jsxSingleQuote": true,
    "jsxBracketSameLine": true,
    "quoteProps": "as-needed",
    "trailingComma": "es5",
    "trailingCommaPHP": false,
    "braceStyle": "1tbs",
    "requirePragma": false,
    "insertPragma": false,
    "arrowParens": "always",
    "semi": true,
    "printWidth": 240,
    "parser": "php",
    "bracketSpacing": true,
    "bracketSameLine": false,
    "withNodeModules": false,
    "proseWrap": "preserve",
    "htmlWhitespaceSensitivity": "css",
    "endOfLine": "auto",
    "embeddedLanguageFormatting": "auto",
    "experimentalTernaries": false
}
